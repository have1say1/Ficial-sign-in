<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.air.facial_sign_in">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:name=".activities.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:networkSecurityConfig="@xml/network_security_config" >

        <activity android:name=".activities.StaticActivity" />
        <activity android:name=".activities.PersonalNoteActivity" />
        <activity android:name=".activities.CamActivity" />
        <activity android:name=".activities.LoginActivity" />
        <activity
            android:name=".activities.WelcomeActivity"
            android:theme="@style/WelcomeStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MessageActivity" />
        <activity android:name=".activities.UserDetailActivity" />
        <activity android:name=".activities.ShareActivity" />
        <activity android:name=".activities.GroupInfoActivity" />
        <activity android:name=".activities.MemberListActivity" />
        <activity android:name=".activities.UserInfoActivity" />
        <activity android:name=".activities.CheckInModeActivity" />
        <activity android:name=".activities.MeetingActivity" />
        <activity android:name=".activities.ForgetActivity" />
        <activity android:name=".activities.Regist1Activity" />
        <activity android:name=".activities.MainActivity"/>
    </application>

</manifest>